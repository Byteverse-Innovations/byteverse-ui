{
  "name": "byteverse-ui",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "build": "npm run codegen && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "deploy": "cdk deploy UIPipelineStack",
    "cdk": "cdk",
    "build:cdk": "tsc",
    "watch:cdk": "tsc -w",
    "cdk synth": "npm run build:cdk && cdk synth",
    "cdk deploy": "npm run build:cdk && cdk deploy",
    "cdk diff": "npm run build:cdk && cdk diff",
    "deploy:prod": "APPSYNC_API_KEY=$APPSYNC_API_KEY cdk deploy UIPipelineStack --context env=prod --context pipeline=true --require-approval never",
    "destroy:prod": "cdk destroy --context env=prod",
    "diff:prod": "cdk diff --context env=prod"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-providers": "^3.967.0",
    "@aws-sdk/protocol-http": "^3.370.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "@aws-sdk/types": "^3.965.0",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "aws-cdk": "^2.1019.1",
    "aws-cdk-lib": "^2.198.0",
    "bootstrap": "^5.3.7",
    "constructs": "^10.4.2",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "postprocessing": "^6.37.7",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "sass": "^1.89.2",
    "source-map-support": "^0.5.21",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@types/node": "^22.16.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
